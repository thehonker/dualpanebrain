{
  "name": "dualpanebrain",
  "displayName": "dualpanebrain",
  "description": "Dual Pane Notebook For OpenAI Compatiable API Interaction",
  "version": "1.0.1",
  "publisher": "thehonker",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other",
    "Machine Learning"
  ],
  "icon": "doc/media/readme/logo.png",
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "id": "dpb",
        "title": "Dual Pane Brain",
        "properties": {
          "dpb.sendFullPromptTextOnContinue": {
            "type": "boolean",
            "default": true,
            "description": "Send full prompt text on continue command",
            "scope": "application"
          },
          "dpb.apiConfigurations": {
            "type": "string",
            "default": null,
            "description": "Container for dpb api configurations",
            "editPresentation": "multilineText",
            "scope": "application"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "dpb.setupApiInstance",
        "title": "Setup Api Instance",
        "category": "Dual Pane Brain"
      },
      {
        "command": "dpb.updateApiKey",
        "title": "Update Api Key for Instance",
        "category": "Dual Pane Brain"
      },
      {
        "command": "dpb.setDefaultInstance",
        "title": "Set Default Api Instance",
        "category": "Dual Pane Brain"
      },
      {
        "command": "dpb.generate",
        "title": "Send contents of left hand editor to LLM as an initial prompt",
        "category": "Dual Pane Brain"
      },
      {
        "command": "dpb.continue",
        "title": "Send contents of both panes to LLM as a continuation prompt",
        "category": "Dual Pane Brain"
      },
      {
        "command": "dpb.stop",
        "title": "Stop current generation",
        "category": "Dual Pane Brain"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
